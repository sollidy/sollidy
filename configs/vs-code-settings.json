{
  "editor.fontSize": 16,
  "explorer.confirmDelete": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "explorer.confirmDragAndDrop": false,
  "git.enableSmartCommit": true,
  "git.autofetch": true,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "git.confirmSync": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "projectManager.git.baseFolders": [
    "Projects",
    "C:\\Code",
    "~/Git/cs",
    "~/Git/my-projects",
    "~/Git/atls"
  ],
  "editor.wordWrap": "on",
  "editor.tabSize": 2,
  "workbench.startupEditor": "none",
  "security.workspace.trust.untrustedFiles": "open",
  "projectManager.openInNewWindowWhenClickingInStatusBar": true,
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": ["<Leader>", "c"],
      "after": ["y"]
    },
    {
      "before": ["<Leader>", "v"],
      "after": ["\"", "*", "p"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "j"],
      "after": ["C-d"]
    },
    {
      "before": ["<leader>", "z"],
      "after": ["z", "z"]
    },
    {
      "before": ["<leader>", "k"],
      "after": ["C-u"]
    },
    {
      "before": ["g", "r"],
      "commands": ["editor.action.goToReferences"]
    },
    {
      "before": ["<leader>", "d"],
      "after": ["d", "d"]
    },
    {
      "before": ["<leader>", "o"],
      "commands": ["whichkey.show"]
    },
    {
      "before": ["<Leader>", "v"],
      "after": ["\"", "*", "p"]
    },
    {
      "before": ["<Leader>", "r"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<Leader>", "s"],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["<Leader>", "h"],
      "after": ["^"]
    },
    {
      "before": ["<Leader>", "l"],
      "after": ["$"]
    },
    {
      "before": ["<Leader>", "q"],
      "after": ["<C-w>", "q"]
    },
    {
      "before": ["<Leader>", ","],
      "after": ["<C-w>", "h"]
    },
    {
      "before": ["<Leader>", "b"],
      "after": ["%"]
    },
    {
      "before": ["<Leader>", "u"],
      "after": ["<C-r>"]
    },
    {
      "before": ["<Leader>", "y"],
      "after": ["y", "i", "w"]
    },
    {
      "before": ["<Leader>", "b"],
      "commands": ["workbench.action.toggleSidebarVisibility"]
    },
    {
      "before": ["<Leader>", "x"],
      "after": ["v", "i", "w"]
    },
    {
      "before": ["<Leader>", "n"],
      "commands": [":noh"]
    },
    {
      "before": ["<Leader>", "d"],
      "after": ["d", "$"]
    },
    {
      "before": ["<Leader>", "f"],
      "after": ["#"]
    },
    {
      "before": ["<Leader>", "t"],
      "commands": ["workbench.action.showCommands"]
    },
    {
      "before": ["<Leader>", "p"],
      "after": ["o", "q", "<BS>", "<Esc>", "p"]
    }
  ],
  "vim.leader": "<space>",
  "vim.useSystemClipboard": true,
  "codesnap.containerPadding": "1em",
  "[sql]": {
    "editor.defaultFormatter": "arniu.sql-formatter-vscode"
  },
  "redhat.telemetry.enabled": false,
  "workbench.preferredLightColorTheme": "Mayukai Reversal",
  "workbench.preferredDarkColorTheme": "Base16 Tomorrow",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorBlinking": "expand",
  "editor.cursorStyle": "line-thin",
  "editor.unicodeHighlight.allowedLocales": {
    "ru": true
  },
  "vim.commandLineModeKeyBindings": [],
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.fontLigatures": true,
  "editor.fontFamily": "JetBrainsMono Nerd Font, JetBrains Mono, Consolas, 'Courier New', monospace",
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "http.proxyAuthorization": null,
  "settingsSync.ignoredExtensions": [],
  "settingsSync.ignoredSettings": [],
  "editor.language.colorizedBracketPairs": [],
  "editor.codeActionsOnSave": {},
  "editor.tokenColorCustomizations": {},
  "npm.packageManager": "yarn",
  "workbench.editorAssociations": {
    "*.gz": "default",
    "*.svg": "default",
    "*.png": "imagePreview.previewEditor"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.associations": {
    ".pnp.cjs": "plaintext",
    "*.scss": "postcss"
  },
  "[dockercompose]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "debug.javascript.autoAttachFilter": "disabled",
  "[xml]": {
    "editor.defaultFormatter": "redhat.vscode-xml"
  },
  "[proto3]": {
    "editor.defaultFormatter": "bufbuild.vscode-buf"
  },
  "protoc": {},
  "diffEditor.ignoreTrimWhitespace": false,
  "git-graph.dialog.rebase.launchInteractiveRebase": true,
  "vim.searchHighlightTextColor": "#6f0",
  "vim.sneak": true,
  "docker.containers.label": "ContainerName",
  "docker.containers.sortBy": "Label",
  "editor.foldingMaximumRegions": 1000,
  "window.titleBarStyle": "custom",
  "window.commandCenter": false,
  "workbench.iconTheme": "bearded-icons",
  "vim.useCtrlKeys": false,
  "window.zoomLevel": 1,
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "gitTreeCompare.diffMode": "merge",
  "gitTreeCompare.showCheckboxes": false,
  "editor.minimap.enabled": false,
  "semanticdiff.closeOriginalTab": true,
  "workbench.colorTheme": "Base16 Tomorrow",
  "projectManager.git.ignoredFolders": [
    "node_modules",
    "out",
    "typings",
    "test",
    ".haxelib",
    "dist"
  ],
  "projectManager.hg.ignoredFolders": [
    "node_modules",
    "out",
    "typings",
    "test",
    ".haxelib",
    "dist"
  ],
  "vs-kubernetes": {
    "vscode-kubernetes.helm-path-linux": "/home/mark/.local/state/vs-kubernetes/tools/helm/linux-amd64/helm"
  },
  "gitlens.ai.experimental.provider": "anthropic",
  "gitlens.ai.experimental.anthropic.model": "claude-2.1",
  "[helm]": {
    "editor.formatOnSave": false
  },
  "editor.formatOnSave": true
}
